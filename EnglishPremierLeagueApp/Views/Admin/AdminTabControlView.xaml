<Window x:Class="EnglishPremierLeagueApp.Views.Admin.AdminTabControlView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EnglishPremierLeagueApp"
             mc:Ignorable="d" 
             d:DesignHeight="700" d:DesignWidth="800">
    <Grid Background="White">
        <TabControl x:Name="UserTabControl" HorizontalAlignment="Left" Height="575" Margin="0,0,-0.4,0" VerticalAlignment="Top" Width="800">
            <TabItem x:Name="MainItem" Header="Table"/>
            <TabItem x:Name="CalendarItem" Header="Calendar"/>
            <TabItem x:Name="NewsItem" Header="News"/>
        </TabControl>
        
    </Grid>
</Window>
